<?xml version="1.0" encoding="iso-8859-1"?>

<root>
    <template name="combobox_eb_save">
        <combobox_eb_ability name="save">
            <script>
                function onInit()
                    super.onInit()
                    add("concentration")
                end
            </script>
        </combobox_eb_ability>
    </template>

    <template name="combobox_eb_creature_type">
        <combobox_eb_valueholder name="creature_type">
            <script>
                function onInit()
                    super.onInit()
                    add("")
                    addItems(DataCommon.creaturetype)
                    addItems(DataCommon.creaturesubtype)
                end
            </script>
            <empty textres="creature_type"/>
        </combobox_eb_valueholder>
    </template>

    <template name="combobox_eb_vision_type">
        <combobox_eb_valueholder name="vision_type">
            <script>
                function onInit()
                    super.onInit()
                    add("")
                    add("darkvision")
                    add("blindsight")
                    add("truesight")
                    add("devilsight")
                end
            </script>
            <empty textres="vision_type"/>
        </combobox_eb_valueholder>
    </template>
</root>
